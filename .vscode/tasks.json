{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "label": "cargo build debug",
      "command": "cargo",
      "args": [
        "build"
      ],
      "problemMatcher": [
        "$rustc"
      ]
    },
    {
      "type": "cargo",
      "label": "cargo build release",
      "command": "cargo",
      "args": [
        "build",
        "--release"
      ],
      "problemMatcher": [
        "$rustc"
      ]
    },
    {
      "type": "cargo",
      "label": "cargo check",
      "command": "cargo",
      "args": [
        "check"
      ],
      "problemMatcher": [
        "$rustc"
      ]
    }
  ]
}